<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="centinel_subgroup_form" model="ir.ui.view">
			<field name="name">Subgroup</field>
			<field name="model">centinel.subgroup</field>
			<field name="arch" type="xml">
				<form string="Subgroup">
					<sheet>
						<field name='name'/>
						<field name='rol'/>
						<field name='start_date'/>
						<field name='end_date'/>
						<field name='vacancy_id'/>
					</sheet>
				</form>
			</field>
		</record>
		<record id="centinel_subgroup_tree" model="ir.ui.view">
			<field name="name">Subgroup</field>
			<field name="model">centinel.subgroup</field>
			<field name="arch" type="xml">
				<tree string="Vacancy post" editable='top'>
					<field name='name'/>
					<field name='rol'/>
					<field name='start_date'/>
					<field name='end_date'/>
					<field name='vacancy_id'/>
				</tree>
			</field>
		</record>
		<record id="action_centinel_subgroup" model="ir.actions.act_window">
			<field name="name">Group</field>
			<field name="res_model">centinel.subgroup</field>
			<field name="view_type">form</field>
			<field name="view_mode">form, tree</field>
		</record>
	</data>
</odoo>